<div class="well">
    {{ form_start(form, {'attr': {'class': 'form-horizontal'}}) }}
    {{ form_errors(form) }}
    <div class="form-group">
        {{ form_label(form.phrase, "Racontez en une phrase amusante cette équipe et décernez lui un prix amusant. ", {'label_attr': {'class': 'col-sm-10 control-label'}}) }}
        {{ form_errors(form.phrase) }}
    </div>
    <div class="form-group">
        {{ form_label(form.phrase, "Phrase amusante", {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
        {{ form_errors(form.phrase) }}
        {{ form_widget(form.phrase, {'attr': {'class': 'col-sm-10 form-control'}}) }}
    </div>
    <div class="form-group">
        {{ form_label(form.liaison, "", {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
        {{ form_errors(form.liaison) }}
        {{ form_widget(form.liaison, {'attr': {'class': 'col-sm-6 form-control'}}) }}
    </div>
    <div class="form-group">
        {{ form_label(form.prix, "Prix amusant", {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
        {{ form_errors(form.prix) }}
        {{ form_widget(form.prix, {'attr': {'class': 'col-sm-10 form-control'}}) }}
    </div>
    <div>
        {{ form_widget(form.Enregistrer, {'attr': {'class': 'btn btn-primary'}}) }}
    </div>
    {{ form_rest(form) }}
    {{ form_end(form) }}
</div>

  