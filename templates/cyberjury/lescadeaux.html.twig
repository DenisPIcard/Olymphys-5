{% extends "base1.html.twig" %}
{% block title %} Les cadeaux {% endblock %}

{% block titre3 %} Les Cadeaux {% endblock %}

{% block navigation %}
    {{ include ('cyberjury/navigation.html.twig') }}
{% endblock %}

{% block body %}

    <div>
        <table class="table table-striped">

            <tr class="danger">
                <th>Cadeau</th>
                <th>Offert par</th>

            </tr>

            {% for cadeaux in ListCadeaux %}
                <tr class="danger">
                    <td> {{ cadeaux.contenu }} </td>
                    <td> {{ cadeaux.fournisseur }} </td>

                </tr>
            {% endfor %}

        </table>
    </div>



{% endblock %}

 

