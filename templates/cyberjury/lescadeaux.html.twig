{% extends "base1.html.twig" %}
{% block title %} Les cadeaux {% endblock %}

{% block titre3 %} Les Cadeaux {% endblock %}

{% block navigation %}
    <button type="button" class="btn btn-outline-primary btn-lg btn-bloc"><span class="fas fa-user">  </span>
        {{ jure.prenomJure|title }} {{ jure.nomJure|title }}
    </button>

    {{ include ('cyberjury/navigation.html.twig') }}
{% endblock %}

{% block body %}

    <div>
        <table class="table table-striped">
            <thead>
            <tr class="danger">
                <th>Cadeau</th>
                <th>Offert par</th>
                <th>Valeur</th>
            </tr>
            </thead>
            <tbody>
            {% for cadeaux in ListCadeaux %}
                <tr class="danger">
                    <td> {{ cadeaux.contenu }} </td>
                    <td> {{ cadeaux.fournisseur }} </td>

                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>



{% endblock %}

 

