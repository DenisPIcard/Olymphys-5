{% extends '@!EasyAdmin/layout.html.twig' %}

{% block main %}
    <div class="rougeodpf">
        <div class="text-center"><h1>Les équipes et leurs documents</h1></div>
    </div>
    <hr>
    {% set urlequipes = ea_url()
        .setDashboard('App\\Controller\\Admin\\DashboardController')
        .setController('App\\Controller\\Admin\\EquipesadminCrudController')
        .setAction('index') %}
    {% set urleleves = ea_url()
        .setDashboard('App\\Controller\\Admin\\DashboardController')
        .setController('App\\Controller\\Admin\\ElevesinterCrudController')
        .setAction('index') %}
    {% set urlprofs = ea_url()
        .setDashboard('App\\Controller\\Admin\\DashboardController')
        .setController('App\\Controller\\Admin\\ProfesseursCrudController')
        .setAction('index') %}
    {% set urletablissements = ea_url()
        .setDashboard('App\\Controller\\Admin\\DashboardController')
        .setController('App\\Controller\\Admin\\EquipesadminCrudController')
        .set('lycees',1)
        .setAction('index') %}
    {#  <a href="{{ path('admin',{crudAction:'index',crudControllerFqcn:'App\\Controller\\Admin\\EquipesadminCrudController',menuIndex:4,signature:'8sSF56PFqugcn0VotqlGKWNrZOy7d6tIJkcbUuMRBgs',submenuIndex:-1}) }}" type="button" class="btn btn-primary">Liste des équipes</a>
        <a href="{{ path('admin',{crudAction:'index',crudControllerFqcn:'App\\Controller\\Admin\\ElevesinterCrudController',menuIndex:5,signature:'8DYt51svWd9zBUvpKqLyOc-igkl9w1cNoT4RIQi5rTw',submenuIndex:-1}) }}" type="button" class="btn btn-primary">Liste des élèves</a>
        <a href="{{ path('admin',{crudAction:'index',crudControllerFqcn:'App\\Controller\\Admin\\ProfesseursCrudController',menuIndex:6,signature:'8iCC6nXEh5A_ooMfS1KdqkfCIlAYamtHUN0V1GECyZE',submenuIndex:-1}) }}" type="button" class="btn btn-primary">Liste des professeurs</a>
        <a href="{{ path('admin',{crudAction:'index',crudControllerFqcn:'App\\Controller\\Admin\\EquipesadminCrudController',menuIndex:6,signature:'8sSF56PFqugcn0VotqlGKWNrZOy7d6tIJkcbUuMRBgs',submenuIndex:-1}) }}" type="button" class="btn btn-primary">Liste des établissements</a>
