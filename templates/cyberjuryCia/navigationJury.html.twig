{% if "now"|date("m/d/Y") >= app.session.get('edition').concourscia|date("m/d/Y") %}
    <li class="nav-item dropdown menu_haut">


        <a class="nav-link" href="{{ path('cyberjuryCia_accueil') }}" role="button" aria-haspopup="true"
           aria-expanded="false">Evaluer une équipe</a>

    </li>
    <li class="nav-item dropdown menu_haut">
        <a class="nav-link" href="{{ path('cyberjuryCia_tableau_de_bord',{'critere':'TOT','sens':'DESC'}) }}"
           role="button"
           aria-haspopup="true"
           aria-expanded="false">Tableau de bord</a>

    </li>
    <li class="nav-item dropdown menu_haut">
        <a class="nav-link dropdown-toggle" href="#" data-toggle="dropdown" role="button"
           aria-haspopup="true" aria-expanded="false">Délibération du jury de {{ app.user.centrecia }}</a>
        <div class="dropdown-menu">
            <a class="dropdown-item login py-0"
               href="{{ path('secretariatjuryCia_classementSousJury',{'centre':app.user.centrecia}) }}">Classement des
                équipes de mon sous-jury
            </a>
            <a class="dropdown-item login py-0"
               href="{{ path('secretariatjuryCia_classement',{'centre':app.user.centrecia}) }}">Classement de toute les
                équipes</a>
            <a class="dropdown-item login py-0"
               href="{{ path('cyberjuryCia_gerer_conseils_equipe',{'centre' : app.user.centrecia.centre}) }}">Voir
                les conseils du jury</a>
        </div>
    </li>
{% endif %}
