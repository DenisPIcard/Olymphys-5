<div class="well">
    {{ form_start(form, {'attr': {'class': 'form-horizontal'}}) }}
    {{ form_errors(form) }}
    <div class="form-group">
        {{ form_label(form.texte, "Ecrivez votre conseil à l'équipe, vous pourrez  lui transmettre après la délibération du concours", {'label_attr': {'class': 'col-sm-10 control-label'}}) }}
        {{ form_errors(form.texte) }}
    </div>
    <div class="form-group">
        {{ form_label(form.texte, "Conseils", {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
        {{ form_errors(form.texte) }}
        {{ form_widget(form.texte, {'attr': {'class': 'col-sm-10 form-control'}}) }}
    </div>

    <div>
        {{ form_widget(form.Enregistrer, {'attr': {'class': 'btn btn-primary'}}) }}

        <button class="btn btn-primary" onclick="history.back();">Annuler</button>


    </div>
    {{ form_rest(form) }}
    {{ form_end(form) }}
</div>

  